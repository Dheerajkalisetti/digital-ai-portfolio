{
    "functions": {
        "server/index.js": {
            "includeFiles": "server/resume.json"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}